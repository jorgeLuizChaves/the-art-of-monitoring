# Global settings
Interval 2
CheckThresholds true
WriteQueueLimitHigh 5000
WriteQueueLimitLow 5000

# Plugin loading
LoadPlugin logfile
LoadPlugin threshold

# Plugin Configuration
<Plugin "logfile">
  LogLevel "info"
  File "/var/log/collectd.log"
  Timestamp true
</Plugin>

Include "/etc/collectd.d/*.conf"